<h1>Login Page</h1><br>

<% if flash.has?(:message) %>
  <p><%= flash[:message] %></p>
<% end %>
<br><br>
<form action="/sessions/customer_login" method="POST">

    <label>Email:</label>
        <input type="text" name="email" id="customer_email"><br><br>
    <label>Password:</label>
        <input type="text" name="password" id="customer_password"><br><br>
        <input type="submit" id="Log In">
<br><br><br>
    <a href="/">Back to Home Page</a>
</form>
<h1>Signup Page</h1><br>

<form action="/customer_signup" method="POST">
    <label>First Name:</label>
    <input type="text" name="customer[first_name]" id="first_name">
    <label>Last Name:</label>
    <input type="text" name="customer[last_name]" id="last_name">
    <label>Email:</label>
    <input type="text" name="customer[email]" id="customer_email">
    <label>Password:</label>
    <input type="text" name="customer[password]" id="customer_password">
    <label>Address:</label>
    <input type="text" name="customer[address]" id="address">
    <label>Phone:</label>
    <input type="text" name="customer[phone_1]" id="phone_1">
    <label>Alt. Phone:</label>
    <input type="text" name="customer[phone_2]" id="phone_2">
    <label>Fax:</label>
    <input type="text" name="customer[fax]" id="fax">
    <input type="submit" id="Sign Up">
</form>